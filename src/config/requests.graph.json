{
  "getUserInfos": {
    "url": "/$batch",
    "body": {
      "requests": [
        {
          "url": "me/photo/$value",
          "method": "GET",
          "id": "photo"
        },
        {
          "url": "me/events?$top=5",
          "method": "GET",
          "id": "events"
        },
        {
          "url": "me/messages?$top=5&$select=webLink,subject,hasAttachments,bodyPreview",
          "method": "GET",
          "id": "messages"
        }
      ]
    }
  }
}
