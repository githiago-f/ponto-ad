{
  "getUserInfos": {
    "url": "https://graph.microsoft.com/v1.0/$batch",
    "body": {
      "requests": [
        {
          "url": "me/photo/$value",
          "method": "GET",
          "id": "1"
        },
        {
          "url": "me/events?$top=5",
          "method": "GET",
          "id": "2"
        },
        {
          "url": "me/messages?$top=5&$select=webLink,subject,hasAttachments,bodyPreview",
          "method": "GET",
          "id": "3"
        }
      ]
    }
  }
}
